<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dockervision.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>__BIN_PATH__</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>~/Library/Logs/dockervision-agent.log</string>
    <key>StandardErrorPath</key>
    <string>~/Library/Logs/dockervision-agent.err.log</string>
</dict>
</plist>
